{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/_layout.jsx",
    "nativewind-env.d.ts",
    "assets/logo.jsx",
    "assets/images/onboarding-image.jsx",
    "app/register.jsx",
    "app/login.jsx",
    "app/onboarding.jsx"
, "app/splashscreen.jsx", "app/index.jsx"  ]
}
