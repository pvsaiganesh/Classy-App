{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/_layout.tsx",
    "nativewind-env.d.ts",
    "assets/logo.jsx",
    "assets/images/onboarding-image.jsx",
    "app/register.tsx",
    "app/login.tsx",
    "app/onboarding.tsx"
, "app/splashscreen.tsx", "app/index.tsx"  ]
}
